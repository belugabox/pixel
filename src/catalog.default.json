{
  "emulators": [
    {
      "id": "retroarch",
      "name": "RetroArch",
      "path": "retroarch.exe",
      "args": ["-f", "-L", "{core}", "{rom}"],
      "coresPath": "cores"
    },
    {
      "id": "m2emulator",
      "name": "Model 2 Emulator",
      "path": "emulator.exe",
      "args": ["-fullscreen", "{romName}"]
    }
  ],
  "tools": [
    {
      "id": "demulshooter",
      "name": "DemulShooter",
      "path": "DemulShooter.exe",
      "args": ["-target={system}", "-rom={romNameLower}"]
    }
  ],
  "systems": [
    {
      "id": "neogeo",
      "name": "Neo Geo",
      "extensions": [".zip", ".neo"],
      "exclude": ["neogeo.zip"],
      "emulator": "retroarch",
      "core": "fbneo_libretro"
    },
    {
      "id": "snes",
      "name": "Super Nintendo",
      "extensions": [".smc", ".sfc", ".fig", ".zip"],
      "emulator": "retroarch",
      "core": "bsnes_libretro"
    },
    {
      "id": "model2",
      "name": "Sega Model 2",
      "extensions": [".zip", ".rom", ".iso"],
      "emulator": "m2emulator",
      "tool": "demulshooter"
    }
  ]
}
