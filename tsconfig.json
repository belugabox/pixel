{
  "compilerOptions": {
    "target": "ESNext",
    "module": "Node16",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "Node16",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "lib": ["ES2021", "DOM"],
    "types": ["vite/client"]
  }
}
